<% if !@product.versions.nil? && @product.versions.length > 1 %>
	<div class="box header">
		<h5>
			<%= pluralize(@product.versions.count, "Versions") %>
		</h5>
	</div>
	<div class="box content" >
		<% @product.get_natural_sorted_versions.each do |version|  %>
			<div>
			<% if ReleaseRecognizer.release?(version.version) %>
				<a href="/package/<%=@product.to_param%>/version/<%=version.to_url_param%>"><%=version.version%></a>
			<% else %>
				<a style="color: brown" href="/package/<%=@product.to_param%>/version/<%=version.to_url_param%>"><%=version.version%></a>
			<% end %>
			</div>
		<% end %>
	</div>
<% end %>