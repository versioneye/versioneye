<% if !signed_in? || dep.nil? || dep.unknown? || (signed_in? && !@project.collaborator?( current_user ) ) %>
  <span>&nbsp;</span>
<% elsif !dep[:muted] %>
  <button class = "btn btn-link btn-mini btn-mute-version mute-off"
          title = "Mute alerts for this version"
          data-project-id = "<%= dep.project[:_id].to_s %>"
          data-language = "<%= dep[:language] %>"
          data-prod-key = "<%= dep[:prod_key] %>">
    <i class = "dep-icon icon-volume-up"></i>
  </button>
<% elsif dep[:muted] %>
  <button class = "btn btn-link btn-mini btn-mute-version mute-on"
          title = "Allow notification for this version."
          data-project-id = "<%= dep.project[:_id].to_s %>"
          data-language = "<%= dep[:language] %>"
          data-prod-key = "<%= dep[:prod_key] %>">
    <i class = "dep-icon icon-volume-off"></i>
  </button>
<% end %>
